<local:FrmODBase x:Class="OpenDental.FrmFaqPicker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:OpenDental"
             xmlns:ui="clr-namespace:WpfControls.UI"
             mc:Ignorable="d"
             Width="772" Height="362" 
             Text="FAQ Picker" >
	<Grid Background="#FFFCFDFE" >
		<ui:Grid x:Name="gridODMain" Width="655" Height="283" Margin="14,46,0,0" Title="FAQs" />
		<ui:CheckBox x:Name="checkShowUnlinked" Width="268" Height="17" Margin="14,335,0,0" Text="Show Unlinked FAQs (Does not consider filters)" CheckAlign="MiddleLeft" />
		<ui:Button x:Name="butAdd" Width="74" Height="23" Margin="684,173,0,0" Text="_Add" Click="ButAdd_Click" />
		<ui:Button x:Name="butRefresh" Width="75" Height="23" Margin="684,44,0,0" Text="Refresh" Click="ButRefresh_Click" />
		<ui:TextBox x:Name="textManualVersion" Width="100" Height="20" Margin="472,15,0,0" />
		<ui:TextBox x:Name="textManualPage" Width="196" Height="20" Margin="132,15,0,0" />
		<ui:Label x:Name="label2" Width="90" Height="14" Margin="376,18,0,0" Text="Manual Version:" HAlign="Right" VAlign="Center" />
		<ui:Label x:Name="label1" Width="127" Height="23" Margin="0,18,0,0" Text="Manual Page Name:" HAlign="Right" />
	</Grid>
</local:FrmODBase>